{
  "autoload": {
    "psr-4": {
      "App\\": "src/",
      "Test\\Unit\\": "tests/unit/"
    }
  },
  "repositories": [
    {
      "type": "git",
      "url": "git@github.com:efremovigor/php-dependency-injection.git"
    },
    {
      "type": "git",
      "url": "git@github.com:efremovigor/php-serializer.git"
    },
    {
      "type": "git",
      "url": "git@github.com:efremovigor/php-dynamic-service.git"
    }
  ],
  "require": {
    "php": "7.2.*",
    "php-dependency-injection": "1.0.*",
    "php-serializer": "1.0.*",
    "php-dynamic-service": "1.0.*",
    "ext-dom": "*",
    "ext-libxml": "*",
    "ext-iconv": "*",
    "ext-mbstring": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "8.3.4"
  }
}